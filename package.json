  "dependencies": {
    "@substreams/core": "^0.1.9",
    "@substreams/node": "^0.1.1",
    "@wharfkit/session": "^0.5.0",
    "commander": "latest",
    "dotenv": "latest",
    "nanoid": "latest"
  },
  "devDependencies": {
    "@tsconfig/recommended": "latest",
    "@types/node": "latest",
    "typescript": "latest"
  }
}
