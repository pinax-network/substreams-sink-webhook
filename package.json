{
  "private": true,
  "name": "substreams-sink-webhook",
  "description": "Substreams Sink Webhook",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "worker": "node ./dist/bin/cli.js worker",
