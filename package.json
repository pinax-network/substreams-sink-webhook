{
  "version": "0.7.3",
  "name": "substreams-sink-webhook",
  "description": "Substreams Sink Webhook",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "substreams-sink-webhook": "dist/bin/cli.js"
  },
  "author": {
    "name": "Denis",
    "email": "denis@pinax.network"
  },
  "files": ["dist"],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "default": "./dist/src/auth/index.js",
      "import": "./dist/src/auth/index.js",
      "types": "./dist/src/auth/index.d.ts"
    }
  },
  "scripts": {
    "start": "tsc && node ./dist/bin/cli.js run",
    "pretest": "tsc --noEmit",
    "test": "bun test",
    "prepublishOnly": "tsc",
    "build": "bun build --compile ./index.ts --outfile substreams-sink-webhook",
    "bench": "bun ./src/**/*.bench.ts"
  },
  "dependencies": {
    "@substreams/sink-database-changes": "^0.3.0",
    "@substreams/sink-entity-changes": "^0.3.4",
    "p-queue": "latest",
    "substreams-sink": "^0.13.4",
    "tweetnacl": "latest",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "bun-types": "latest",
    "mitata": "latest",
    "typescript": "latest"
  }
}
