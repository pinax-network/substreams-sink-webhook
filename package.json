{
  "version": "0.8.0",
  "name": "substreams-sink-webhook",
  "description": "Substreams Sink Webhook",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "substreams-sink-webhook": "dist/bin/cli.js"
  },
  "author": {
    "name": "Denis",
    "email": "denis@pinax.network"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "default": "./dist/src/auth/index.js",
      "import": "./dist/src/auth/index.js",
      "types": "./dist/src/auth/index.d.ts"
    }
  },
  "scripts": {
    "start": "tsc && node ./dist/bin/cli.js run --help",
    "pretest": "tsc --noEmit",
    "test": "bun test",
    "posttest": "bunx @biomejs/biome check .",
    "prepublishOnly": "tsc",
    "format": "bunx @biomejs/biome format --write ./",
    "build": "bun build --compile ./index.ts --outfile substreams-sink-webhook",
    "bench": "bun ./src/**/*.bench.ts"
  },
  "dependencies": {
    "@substreams/sink-database-changes": "^0.3.2",
    "@substreams/sink-entity-changes": "^0.3.4",
    "hono": "^4.0.1",
    "p-queue": "latest",
    "substreams-sink": "^0.14.0",
    "tweetnacl": "latest",
    "viem": "^2.7.8",
    "zod": "latest"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "bun-types": "latest",
    "mitata": "latest",
    "typescript": "latest"
  }
}
